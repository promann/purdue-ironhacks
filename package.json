{
  "name": "bloggify-custom-app-template",
  "version": "1.0.2",
  "description": "A custom application built with @Bloggify.",
  "main": "bloggify.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=production bloggify start --no-bundle",
    "start:dev": "bloggify start",
    "bundle": "PORT=8081 NODE_ENV=production bloggify start --exit-after-bundle",
    "start:debug": "node debug `which bloggify` start",
    "deploy": "./scripts/heroku-deploy.sh",
    "enable-heroku-cache": "heroku config:unset NODE_MODULES_CACHE",
    "disable-heroku-cache": "heroku config:set NODE_MODULES_CACHE=false"
  },
  "author": "Ionică Bizău <bizauionica@gmail.com> (http://ionicabizau.net)",
  "license": "MIT",
  "dependencies": {
    "bag.css": "^1.0.5",
    "bloggify-cli": "^1.1.1",
    "bloggify-config": "^1.0.6",
    "bloggify-custom-assets": "^1.0.2",
    "bloggify-flexible-router": "^1.0.7",
    "bloggify-github-login": "^1.0.0",
    "bloggify-mongoose": "^1.0.2",
    "bloggify-router": "^1.0.7",
    "bloggify-sendgrid": "^1.1.0",
    "codemirror": "^5.24.0",
    "elly": "^1.1.3",
    "fast-csv": "^2.3.1",
    "gridly": "^1.4.8",
    "jquery": "^2.2.4",
    "modernizr": "^3.3.1",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "node-schedule": "^1.2.0",
    "obj-flatten": "^2.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-markdown": "^2.4.4",
    "react-md-editor": "^0.2.2",
    "regex-escape": "^3.4.4",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "unique-random-range": "^1.0.1",
    "whatwg-fetch": "^2.0.2"
  },
  "files": [
    "bin/",
    "app/",
    "lib/",
    "dist/",
    "src/",
    "resources/",
    "menu/",
    "scripts/",
    "cli.js",
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/BloggifyTutorials/custom-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BloggifyTutorials/custom-app.git"
  },
  "homepage": "https://github.com/BloggifyTutorials/custom-app#readme",
  "bloggify": {
    "bundles": {
      "publicHomepage": {
        "scripts": [
          "app/assets/javascripts/homepage/index.js"
        ],
        "styles": [
          "app/assets/stylesheets/homepage/index.css"
        ]
      },
      "auth": {
        "scripts": [
          "app/assets/javascripts/private-router.js"
        ],
        "styles": [
          "app/assets/stylesheets/auth.css"
        ]
      },
      "notAuth": {
        "scripts": [
          "app/assets/javascripts/public-router.js"
        ]
      },
      "main": {
        "scripts": [
          "app/assets/javascripts/common-router.js"
        ]
      }
    }
  }
}
